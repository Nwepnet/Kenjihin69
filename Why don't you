local playerGui = player:WaitForChild("PlayerGui")
local screenGui = playerGui:WaitForChild("ScreenGui")
local magicHealth = screenGui:WaitForChild("MagicHealth")
local textLabel = magicHealth:WaitForChild("TextLabel")

